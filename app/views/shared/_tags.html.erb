<div class="full-width-box articles-navigation">
  <div class="wrapper border-top">
    <div class="inner center">
      <div class="tags">
        Tags: <%= raw Tag.all.map { |tag| link_to tag.title, tag }.join(", ") %>
      </div>
      <div class="archive">
        <%= link_to "Archive", archive_path %>
      </div>
      <div class="clear"></div>
    </div>
  </div>
</div>
